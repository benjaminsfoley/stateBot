{
  "name": "statebot",
  "version": "0.1.0",
  "description": "A deterministic state manager for SvelteKit leveraging LLMs",
  "author": "https://xbg.solutions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/statebot.git"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "state-management",
    "llm",
    "ai",
    "machine-learning"
  ],
  "type": "module",
  "main": "dist/statebot.umd.cjs",
  "module": "dist/statebot.js",
  "types": "dist/index.d.ts",
  "svelte": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/statebot.js",
      "require": "./dist/statebot.umd.cjs"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}